        <dependency>
            <groupId>com.ruoyi</groupId>
            <artifactId>xy-news</artifactId>
        </dependency>

        <!-- 企业库-->
        <dependency>
            <groupId>com.ruoyi</groupId>
            <artifactId>xy-enterprise</artifactId>
        </dependency>

        <!-- 供需-->
        <dependency>
            <groupId>com.ruoyi</groupId>
            <artifactId>xy-supply</artifactId>
        </dependency>

        <!-- COMMON-->
        <dependency>
            <groupId>com.ruoyi</groupId>
            <artifactId>ruoyi-common</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.hutool</groupId>
            <artifactId>hutool-all</artifactId>
            <version>5.4.0</version>
        </dependency>
